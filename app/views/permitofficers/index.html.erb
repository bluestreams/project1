<h1>Listing permitofficers</h1>

<table>
  <thead>
    <tr>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Middlename</th>
      <th>Username</th>
      <th>Hashed password</th>
      <th>Salt</th>
      <th>National id no</th>
      <th>Role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @permitofficers.each do |permitofficer| %>
      <tr>
        <td><%= permitofficer.firstname %></td>
        <td><%= permitofficer.lastname %></td>
        <td><%= permitofficer.middlename %></td>
        <td><%= permitofficer.username %></td>
        <td><%= permitofficer.hashed_password %></td>
        <td><%= permitofficer.salt %></td>
        <td><%= permitofficer.national_id_no %></td>
        <td><%= permitofficer.role_id %></td>
        <td><%= link_to 'Show', permitofficer %></td>
        <td><%= link_to 'Edit', edit_permitofficer_path(permitofficer) %></td>
        <td><%= link_to 'Destroy', permitofficer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Permitofficer', new_permitofficer_path %>
